{{#unless groupsOf}}
    {{#each items as |item index|}}
        {{yield item}}
    {{/each}}
{{else}}
    {{#each groups as |group|}}
        {{#groups-of items=group tagName=groupTag groupsOf=0 as |item|}}
            {{yield item.item item.isFirst item.isLast}}
        {{/groups-of}}
    {{/each}}
{{/unless}}
